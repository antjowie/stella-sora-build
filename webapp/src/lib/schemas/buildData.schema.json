{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "BuildData",
  "type": "object",
  "required": ["name", "description", "id", "potentialIds", "editMode"],
  "additionalProperties": false,
  "properties": {
    "name": {
      "type": "string"
    },
    "description": {
      "type": "string"
    },
    "id": {
      "type": "string"
    },
    "mainId": {
      "type": "integer"
    },
    "support1Id": {
      "type": "integer"
    },
    "support2Id": {
      "type": "integer"
    },
    "potentialIds": {
      "type": "array",
      "items": {
        "type": "integer"
      }
    },
    "editMode": {
      "type": "boolean"
    }
  }
}
