{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "definitions": {
    "PotentialConfig": {
      "type": "object",
      "properties": {
        "level": {
          "type": "number"
        },
        "priority": {
          "type": "number"
        }
      },
      "additionalProperties": {}
    },
    "BuildData": {
      "type": "object",
      "properties": {
        "name": {
          "type": "string"
        },
        "description": {
          "type": "string"
        },
        "id": {
          "type": "string"
        },
        "mainId": {
          "type": "number"
        },
        "support1Id": {
          "type": "number"
        },
        "support2Id": {
          "type": "number"
        },
        "potentialIds": {
          "type": "array",
          "items": {
            "type": "number"
          }
        },
        "potentialConfigs": {
          "type": "array",
          "items": {
            "type": "array",
            "minItems": 2,
            "items": [
              {
                "type": "number"
              },
              {
                "$ref": "#/definitions/PotentialConfig"
              }
            ],
            "maxItems": 2
          }
        },
        "levelMap": {
          "type": "array",
          "items": {
            "type": "array",
            "items": {
              "type": "number"
            },
            "minItems": 2,
            "maxItems": 2
          }
        },
        "editMode": {
          "type": "boolean"
        }
      },
      "required": [
        "name",
        "description",
        "id",
        "potentialIds",
        "editMode"
      ],
      "additionalProperties": {}
    }
  }
}